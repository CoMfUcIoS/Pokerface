{
  "name": "pokerface",
  "version": "1.0.0",
  "author": "Ioannis Karasavvaidis",
  "license": "ISC",
  "description": "Code challenge Poker face",
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.9.0"
  },
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint --ext .jsx --ext .js --ext .json src",
    "tests": "mocha  --require @babel/polyfill --require @babel/register --require ignore-styles --require mocha-snapshots --require ./test/setup.js \"./src/**/*.test.{js,jsx}\" --exit",
    "test:watch": "mocha  --require @babel/polyfill --require @babel/register --require ignore-styles --require mocha-snapshots --require isomorphic-fetch --require ./test/setup.js --exit",
    "test": "npm run build && nyc mocha  \"./src/**/*.test.{js,jsx}\" --exit"
  },
  "dependencies": {
    "@emotion/core": "^10.0.21",
    "@emotion/styled": "^10.0.17",
    "@karlandin/playing-cards": "^0.3.0",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-card-flip": "^0.8.1",
    "react-dom": "^16.10.2",
    "readline": "^1.3.0",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "15.2.0",
    "jsdom-global": "3.0.2",
    "mocha": "^6.2.2",
    "mocha-snapshots": "^4.2.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "style-loader": "^1.0.0",
    "supertest": "^4.0.2",
    "terser-webpack-plugin": "^2.2.1",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  }
